{
  "name": "angrplayr",
  "description": "Console music player in Node.",
  "version": "1.2.9",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "lib",
    "index.js"
  ],
  "preferGlobal": true,
  "bin": "./index.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "cover": "nyc npm run test:tape",
    "demo": "babel-node src/index.js",
    "prebuild": "npm test",
    "preinstall": "hash mplayer 2>/dev/null || { echo >&2 'Please install mplayer.' ; exit 1 ; }",
    "preversion": "npm run build",
    "start": "node index",
    "test": "npm run test:lint && npm run cover",
    "test:lint": "eslint -c .eslintrc.json src",
    "test:tape": "babel-tape-runner src/*.test.js | tap-spec"
  },
  "homepage": "https://github.com/zacanger/angrplayr#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/angrplayr.git"
  },
  "bugs": "https://github.com/zacanger/angrplayr/issues",
  "keywords": [
    "angrplayr",
    "audio",
    "console",
    "mp3",
    "music",
    "player",
    "terminal",
    "zacanger"
  ],
  "dependencies": {
    "babel-register": "6.26.0",
    "blessed": "0.1.81",
    "mime": "2.3.1",
    "mplayer": "2.1.0",
    "music-metadata": "2.0.3",
    "react": "16.4.1",
    "react-blessed": "0.2.1",
    "react-blessed-contrib": "0.2.1",
    "zeelib": "7.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-latest-minimal": "1.1.2",
    "babel-preset-react": "6.24.1",
    "babel-tape-runner": "2.0.1",
    "eslint": "4.19.1",
    "eslint-config-zacanger": "3.1.4",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.9.1",
    "eslint-plugin-unicorn": "4.0.3",
    "nyc": "12.0.2",
    "tap-spec": "5.0.0",
    "tape": "4.9.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ]
  }
}
